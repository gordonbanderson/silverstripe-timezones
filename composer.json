{
  "name": "suilven/silverstripe-timezones",
  "type": "silverstripe-vendormodule",
  "description": "Helpers to convert dates between different timezones for client presentation purposes.",
  "homepage": "https://github.com/gordonbanderson/silverstripe-timezones",
  "keywords": ["SilverStripe","timezones","time"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Gordon Anderson",
      "role": "Developer",
      "email": "gordon.b.anderson@gmail.com",
      "homepage": "https://www.github.com/gordonbanderson"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "silverstripe/framework": "^4@dev",
    "nesbot/carbon": "^1."
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Suilven\\TimeZones\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "expose": [
      "admin/client/dist",
      "client/dist"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
